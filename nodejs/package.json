  "devDependencies": {
    "@types/node": "^22.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "fix-esm-import-path": "^1.4.0",
    "openapi-typescript-codegen": "^0.25.0",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.10.3",
    "typescript": "^5.2.2"
